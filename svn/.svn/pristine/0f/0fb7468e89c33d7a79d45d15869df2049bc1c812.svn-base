.container {
  width: 750rpx;
  padding: 30rpx;
  background: #fff;
}

.container .item {
  height: 100rpx;
  width: 690rpx;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
  align-items: center;
}

.container .item .key {
  margin-right: 30rpx;
}

.container .height {
  height: 140rpx;
}

.default .item {
  padding: 0 30rpx;
  height: 160rpx;
  margin-top: 30rpx;
  background: #fff;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.input {
  color: #999;
}

.key {
  font-size: 34rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.button {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: rgba(250, 190, 0, 1);
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  position: fixed;
  bottom: 0;
}
