  font-family: "iconfont";
  src: url('iconfont.eot?t=1573097927774'); /* IE9 */
  src: url('iconfont.eot?t=1573097927774#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
         url('iconfont.woff?t=1573097927774') format('woff'),
         url('iconfont.ttf?t=1573097927774') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+ */ url('iconfont.svg?t=1573097927774#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 32rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-fanhui:before {
  content: "\e618";
}

.icon-xuanze:before {
  content: "\e627";
}

.icon-dianpu:before {
  color: #999;
  content: "\e60b";
}
