/**
 * webview
 * 
 * @author huoyujia
 * @date 2019-08-27
 */

Page({

  /**
   * 页面的初始数据
   */
  data: {
    webUrl: '',
  },

  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function(options) {
    var that = this
    var url = options.url;
    url = decodeURIComponent(url);
    that.setData({
      webUrl: url,
    })
  },
})