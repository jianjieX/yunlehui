page {
  background: #f0f0f0;
}

.page {
  width: 100%;
  padding: 0rpx 0rpx 32rpx 0rpx;
  box-sizing: border-box;
}

.sale-status {
  width: 100%;
  height: 192rpx;
  background: #fabe00;
  padding: 40rpx 0 0 40rpx;
  box-sizing: border-box;
}

.sale-status .status-text {
  width: 320rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 44rpx;
}

.sale-status .count-down-time{
  margin-top: 16rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: rgba(255, 255, 255, 1);
}

.sale-shop {
  width: 100%;
  background: #fff;
  padding: 24rpx 24rpx 16rpx 24rpx;
  box-sizing: border-box;
}

.sale-shop .sale-title {
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 88, 80, 1);
  line-height: 40rpx;
}

.sale-shop .sale-desc {
  margin-top: 24rpx;
  height: 96rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 32rpx;
  margin-bottom: 24rpx;
}

.sale-status .shop-title {
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
}

.sale-goods {
  margin-top: 32rpx;
  width: 100%;
  min-height: 296rpx;
  background: rgba(240, 240, 240, 1);
  border-radius: 8rpx;
  padding: 24rpx 24rpx 18rpx 24rpx;
  box-sizing: border-box;
}

.sale-goods .shopcard {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  flex-direction: row;
}

.sale-goods .shopcard .shop {
  font-size: 28rpx;
  line-height: 40rpx;
}

.sale-goods .shopcard .shop-name {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333333;
  margin-left: 18rpx;
  margin-top: -2rpx;
  line-height: 40rpx;
}

.sale-goods .page-item {
  margin-top: 26rpx;
  width: 100%;
  box-sizing: border-box;
  margin: 16rpx 0 0 0;
  display: flex;
  flex-direction: column;
}

.goods-item {
  width: 100%;
  min-height: 170rpx;
  display: flex;
}

.goods-item .goods-img {
  width: 160rpx;
  height: 160rpx;
  vertical-align: middle;
  border-radius:4rpx;
  margin-right: 24rpx;
}

.goods-img image {
  width: 160rpx;
  height: 160rpx;
}

.goods-item .goods-info {
  flex: 1;
  height: 160rpx;
  display: flex;
  flex-direction: row;
}

.goods-title {
  display: flex;
  flex-direction: column;
}

.goods-title .title {
  width: 352rpx;
  height: 68rpx;
  font-size: 22rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.goods-title .desc {
  margin-top: 16rpx;
  height: 30rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 30rpx;
}

.price-num-view {
  width: 120rpx;
  height: 160rpx;
  display: flex;
  flex-direction: column;
  text-align: right;
}

.price-num-view .price {
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.price-num-view .num {
  height: 34rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
}

.page-item .total-price {
  width: 100%;
  height: 34rpx;
  display: flex;
  font-size: 20rpx;
  color: rgba(255, 88, 80, 1);
  flex-direction: row;
  justify-content: flex-end;
}

.page-item .total-price .no {
  height: 34rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 34rpx;
}

.page-item .total-price .price {
  height: 34rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 88, 80, 1);
  line-height: 34rpx;
}

