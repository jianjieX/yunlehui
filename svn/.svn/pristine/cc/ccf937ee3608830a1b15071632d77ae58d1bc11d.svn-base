/* pages/applyAfterSale/applyAfterSale.wxss */

page {
  background: #f0f0f0;
}

.page {
  
  border-top: 1rpx solid #dcdcdc;
  width: 100%;
  padding: 24rpx 22rpx 32rpx 22rpx;
  box-sizing: border-box;
}

.head {
  text-align: center;
  font-size: 22rpx;
  line-height: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.head .head-text {
  color: #ff5850;
}

.page-item {
  width: 100%;
  background: #fff;
  border-radius: 8rpx;
  padding: 24rpx 24rpx 18rpx 24rpx;
  box-sizing: border-box;
  margin: 16rpx 0;
}

.goods-item {
  width: 100%;
  height: 170rpx;
  display: flex;
}

.goods-item .goods-img {
  width: 160rpx;
  height: 160rpx;
  vertical-align: middle;
  margin-right: 24rpx;
}

.goods-img image {
  width: 160rpx;
  height: 160rpx;
}

.goods-item .goods-info {
  flex: 1;
  height: 160rpx;
  display: flex;
  flex-direction: row;
}

.goods-title {
  display: flex;
  flex-direction: column;
}

.goods-title .title {
  width: 352rpx;
  height: 68rpx;
  font-size: 22rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.goods-title .desc {
  margin-top: 16rpx;
  height: 30rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 30rpx;
}

.price-num-view {
  width: 120rpx;
  height: 160rpx;
  display: flex;
  flex-direction: column;
  text-align: right;
}

.price-num-view .price {
  width: auto;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.price-num-view .num {
  height: 34rpx;
  font-size: 22rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
}

.page-item .total-price {
  width: 100%;
  height: 34rpx;
  display: flex;
  font-size: 20rpx;
  color: rgba(255, 88, 80, 1);
  flex-direction: row;
  justify-content: flex-end;
}

.page-item .total-price .no {
  height: 34rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 34rpx;
}

.page-item .total-price .price {
  height: 34rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 88, 80, 1);
  line-height: 34rpx;
}

.after-detail {
  width: 100%;
  height: 164rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 0 24rpx 0 24rpx;
  box-sizing: border-box;
}

.after-detail  .modes, .total {
  width: 100%;
  height: 82rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 82rpx;
}

.after-detail  .modes:first-of-type {
  border-bottom: 2rpx solid #f5f5f5;
}

.after-detail  .modes .title, .total .title {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 82rpx;
}

.after-detail  .modes .mode, .total .price {
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 82rpx;
}

.after-reason {
  margin-top: 16rpx;
  width: 100%;
  height: 348rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 0 24rpx 0 24rpx;
  box-sizing: border-box;
}

.after-reason .reason {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 82rpx;
}

.after-reason .reason .title {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 82rpx;
}

.after-reason .reason .detail {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 82rpx;
}

.after-reason .texat {
  height: 240rpx;
}

.after-reason .reason .detail .arrow {
  width: 13rpx;
  height: 13rpx;
  border-bottom: 2rpx solid #666;
  border-left: 2rpx solid #666;
  display: inline-block;
  transform: rotate(225deg);
  margin-bottom: 2rpx;
}

textarea {
  background: rgba(244, 244, 244, 1);
  width: 100%;
  height: 300rpx;
  padding: 16rpx 24rpx;
  box-sizing: border-box;
  color: #999;
  font-size: 26rpx;
  position: relative;
}

.input-count {
  position: absolute;
  right: 30rpx;
  bottom: 20rpx;
  z-index: 9;
}

input {
  height: 104rpx;
  line-height: 104rpx;
  text-align: right;
  color: #222;
}

.after-image {
  margin-top: 16rpx;
  width: 100%;
  min-height: 222rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 0 24rpx 30rpx 24rpx;
  box-sizing: border-box;
}

.after-image .title {
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 82rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 82rpx;
  border-bottom: 2rpx solid #f5f5f5;
}

.UpPick {
  display: flex;
  flex-wrap: wrap;
  margin-top: 20rpx;
}

.addPics {
  width: 140rpx;
  height: 140rpx;
  border: 2rpx dashed rgba(226, 226, 226, 1);
  margin-left: 8rpx;
  margin-right: 8rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.addPicPic {
  width: 140rpx;
  height: 140rpx;
  border: 2rpx dashed rgba(226, 226, 226, 1);
  margin-right: 20rpx;
  margin-bottom: 20rpx;
  position: relative;
}

.camera {
  width: 36rpx;
  height: 32rpx;
  display: block;
  margin: 16rpx auto;
}

.cancel {
  position: absolute;
  width: 32rpx;
  height: 32rpx;
  right: -10rpx;
  top: -10rpx;
  z-index: 999;
}

.smal {
  font-size: 24rpx;
  text-align: center;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 32rpx;
}

.moresmal {
  font-size: 20rpx;
  text-align: center;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 24rpx;
}

.slide-pic {
  width: 140rpx;
  height: 140rpx;
}

.contact {
  margin-top: 16rpx;
  width: 100%;
  height: 164rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 0 24rpx 0 24rpx;
  box-sizing: border-box;
}

.contact .person, .mobile {
  width: 100%;
  height: 82rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 82rpx;
}

.contact  .person:first-of-type {
  border-bottom: 2rpx solid #f5f5f5;
}

.contact .person .title, .mobile .title {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 82rpx;
}

.contact  .person .name, .mobile .num {
  height: 82rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 82rpx;
}

