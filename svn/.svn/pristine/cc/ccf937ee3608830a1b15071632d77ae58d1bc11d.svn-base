.submit-btn {
  margin: 48rpx 0 0 0;
  width: 100%;
  height: 88rpx;
  line-height: 88rpx;
  border-radius: 8rpx;
  background: #FABE00;
  text-align: center;
  z-index: 999;
  font-size: 36rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

