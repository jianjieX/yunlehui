/* 编辑地址页面css */

.container {
  width: 750rpx;
  padding: 30rpx;
  background: #fff;
}

.container .item {
  height: 100rpx;
  width: 690rpx;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
  align-items: center;
}

.container .item .key {
  margin-right: 30rpx;
}

.container .height {
  height: 140rpx;
}

.default .item {
  padding: 0 30rpx;
  height: 160rpx;
  margin-top: 30rpx;
  background: #fff;
  border-bottom: 1px solid #f1f1f1;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.button {
  width: 200rpx;
  transform: translateX(-50%);
  margin-left: 50%;
  margin-top: 50rpx;
  padding: 0 160rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: #f4bb2e;
  color: #fff;
  text-align: center;
  border-radius: 80rpx;
}
