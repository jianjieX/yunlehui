.is_checked {
  filter: blur(10rpx);
  width: 100%;
  height: 100%;
}

page {
  height: 100%;
}

/* 导航栏样式 */

.header {
  width: 750rpx;
  background: white;
  font-size: 32rpx;
  box-sizing: border-box;
  position: fixed;
  top: 0;
  z-index: 999;
}

.header .navigation {
  padding-top: 12rpx;
  padding-left: 30rpx;
  display: flex;
}

.arrow-view {
  width: 5%;
  height: 100%;
  left: 0;
  z-index: 999;
  position: relative;
}

.label {
  margin-left: 16rpx;
  color: #333;
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
}

.arrow {
  width: 22rpx;
  height: 22rpx;
  border-bottom: 2px solid black;
  border-left: 2px solid black;
  position: absolute;
  transform: rotate(45deg);
  top: 18rpx;
  left: 2rpx;
}

.normoal {
  box-sizing: border-box;
}

.can-withdraw-view {
  width: 702rpx;
  height: 600rpx;
  line-height: 104rpx;
  background: rgba(255, 255, 255, 1);
  margin: 32rpx auto;
  z-index: 0;
}

.can-withdraw-text {
  display: flex;
  flex-direction: column;
  font-family: PingFangSC-Regular;
  color: rgba(102, 102, 102, 1);
}

.can-withdraw-money {
  width: 260rpx;
  height: 64rpx;
  margin: 80rpx auto;
  font-size: 72rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  color: #333;
}

.can-withdraw-text text {
  font-size: 28rpx;
  margin: -69rpx auto;
  font-family: PingFangSC-Regular;
  color: #333;
}

.tip-text {
  width: 120rpx;
  height: 34rpx;
  line-height: 24rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  font-weight: 600;
  margin: 72rpx 24rpx 0 24rpx;
}

.line-view {
  width: 702rpx;
  height: 2rpx;
  background-color: #eee;
  margin: 0rpx auto;
}

.input {
  display: flex;
  align-items: flex-end;
  width: 100%;
  height: 98rpx;
  margin: 30rpx 0 0 0;
  background: rgba(255, 255, 255, 1);
}

.input input {
  height: 100%;
  flex: 1;
  font-size: 56rpx;
  font-weight: 500;
}

.place-holder {
  font-size: 26rpx;
}

.input .icon {
  font-size: 36rpx;
  font-family: PingFangSC-Medium;
  margin-left: 26rpx;
  color: #333;
  font-weight: 600;
}

.input .all {
  margin: 0 24rpx;
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #ff4b2f;
}

.cardInfo {
  margin-top: 16rpx;
}

.cardInfo view {
  margin: 0 0 -50rpx 0;
}

.cardInfo view .tip {
  font-size: 26rpx;
  color: rgba(51, 51, 51, 1);
  font-family: PingFangSC-Medium;
  margin-left: 24rpx;
  font-weight: 550;
}

.cardInfo view .bankName {
  font-size: 26rpx;
  color: rgba(153, 153, 153, 1);
  margin-left: 74rpx;
}

.cardInfo view .cardHolder {
  font-size: 26rpx;
  color: rgba(153, 153, 153, 1);
  margin-left: 48rpx;
}

.withdraw-rule {
  margin-top: 56rpx;
}

.withdraw-rule text {
  margin-left: 24rpx;
  color: #666;
}

.rules {
  display: flex;
  flex-direction: column;
  margin: 16rpx 0 0 5rpx;
  font-size: 24rpx;
  color: #666;
}

.rules text {
  padding: 0 0 10rpx 0;
}

.btn {
  background: rgba(250, 190, 0, 1);
  width: 702rpx;
  height: 88rpx;
  text-align: center;
  position: absolute;
  bottom: 32rpx;
  border-radius: 8rpx;
  margin-left: 24rpx;
}

.btn-null {
  background: #fabe00;
  width: 702rpx;
  height: 88rpx;
  text-align: center;
  position: absolute;
  bottom: 32rpx;
  border-radius: 8rpx;
  margin-left: 24rpx;
  opacity: 0.6;
}

.btn text {
  line-height: 88rpx;
  font-family: PingFangSC-Regular;
  color: rgba(51, 51, 51, 1);
  font-size: 36rpx;
}

.btn-null text {
  line-height: 88rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Regular;
  color: rgba(153, 153, 153, 1);
}

/* 支付密码框 */

.btn_pay {
  margin: 100rpx auto;
  width: 600rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 100rpx;
  background-color: #d3a95a;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
}

/* 支付密码css start */

.bg_layer {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.6);
  z-index: 9998;
}

.input_main {
  position: fixed;
  left: 35rpx;
  bottom: 500rpx;
  width: 90%;
  height: 394rpx;
  background-color: #fff;
  z-index: 9999;
  border-radius: 18rpx;
}

.input_title {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  font-size: 32rpx;
  border-bottom: 1rpx solid #e2e2e2;
}

.input_back {
  position: absolute;
  left: 0;
  top: 0;
  width: 80rpx;
  height: 90rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.input_back text {
  width: 20rpx;
  height: 20rpx;
  background-color: white;
  border: 1rpx solid #aaa;
  border-width: 5rpx 0 0 5rpx;
  transform: rotate(-45deg);
}

.input_num {
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  font-size: 18px;
  color: #f62e48;
}

.input_tip {
  margin: 20rpx;
  text-align: center;
  font-size: 24px;
  color: #f4bb2e;
}

/* 密码掩码模拟 */

.input_row {
  width: 90%;
  margin: 0 auto;
  height: 98rpx;
  position: relative;
  display: flex;
  align-items: center;
  border: 1rpx solid #e2e2e2;
  border-radius: 20rpx;
}

.input_row .pwd_item {
  flex: 1;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  border-right: 1rpx solid #e2e2e2;
  position: relative;
}

.pwd_item:nth-last-of-type(1) {
  border-right: 0;
}

.pwd_item text {
  width: 30rpx;
  height: 30rpx;
  border-radius: 30rpx;
  background-color: #555;
}

.forget_pwd {
  float: right;
  margin: 30rpx;
  width: 200rpx;
  text-align: right;
  font-size: 24rpx;
  color: #f4bb2e;
}

/* 文本输入框位置: 设置到左边隐藏 */

.input_control {
  position: relative;
  left: -300rpx;
  bottom: 0;
  width: 100rpx;
  height: 100rpx;
}

/* 提现成功 */

.success-view {
  text-align: center;
  margin-top: -1100jj,rpx;
  position: fixed;
  left: 100rpx;
  z-index: 1;
}

.success-text {
  display: flex;
  flex-direction: row;
  font-size: 24rpx;
  justify-content: center;
  margin-top: 72rpx;
  color: #333;
}

.success-text .success-text1 {
  color: #fabe00;
  font-family: PingFang-SC-Semibold;
}

.success-text3 {
  font-size: 24rpx;
  color: #333;
  font-family: PingFang-SC-Medium;
}

.success-button {
  width: 236rpx;
  height: 76rpx;
  line-height: 76rpx;
  background: #fabe00;
  margin: 48rpx auto 0 auto;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 500;
  color: #333;
}
