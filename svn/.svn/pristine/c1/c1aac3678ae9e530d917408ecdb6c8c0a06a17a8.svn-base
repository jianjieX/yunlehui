/* 购物车css */

page {
  padding-bottom: 100rpx;
}

#shopCar {
  box-sizing: border-box;
  margin: 24rpx;
}

.top-img-style {
  width: 100%;
  position: fixed;
  top: 0;
  z-index: -99;
}

#shopCar .shop {
  width: 100%;
  background: #fff;
  overflow: hidden;
  margin-bottom: 30rpx;
}

#shopCar .shopName {
  margin-top: 20rpx;
  margin-left: 24rpx;
  position: relative;
}

.merchantName {
  width: 400rpx;
  display: inline-block;
  margin-left: 8rpx;
  position: relative;
  font-size: 16px;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(34, 34, 34, 1);
  line-height: 20px;
  overflow: hidden;
  text-overflow: ellipsis;
  vertical-align: middle;
  white-space: nowrap;
}

#shopCar .shopImg {
  width: 32rpx;
  height: 32rpx;
  margin-left: 24rpx;
  vertical-align: middle;
}

#shopCar .shopName .arrow {
  width: 7px;
  height: 7px;
  border-top: 2px solid #999;
  border-right: 2px solid #999;
  position: absolute;
  right: -30rpx;
  top: 16rpx;
  transform: rotate(45deg);
}

#shopCar .goodsItem {
  height: 240rpx;
  background: #fff;
  display: flex;
  align-items: center;
  position: relative;
}

#shopCar .goodsItem .check-group {
  margin: 0 24rpx;
  color: #f62e48;
}

#shopCar .goodsItem .goodsImg {
  width: 160rpx;
  height: 160rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #f9f9f9;
  overflow: hidden;
}

#shopCar .goodsItem .goodsImg image {
  width: 100%;
  height: 100%;
  border-radius: 4rpx;
}

#shopCar .goodsItem .goodsInfo {
  margin-left: 24rpx;
  flex: 1;
  padding-right: 24rpx;
}

#shopCar .goodsItem .goodsInfo .title {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  font-size: 13px;
  color: #333;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
}

#shopCar .goodsItem .goodsInfo .select {
  vertical-align: middle;
  display: inline-block;
  width: 64rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  margin-right: 10rpx;
  margin-top: -5rpx;
  background: rgba(255, 88, 80, 1);
  border-radius: 0px 2px 0px 2px;
  font-size: 11px;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

#shopCar .goodsItem .goodsInfo .spec {
  display: inline-block;
  height: 32rpx;
  line-height: 32rpx;
  font-size: 11px;
  color: #666;
  box-sizing: border-box;
  text-align: center;
  padding: 0rpx 20rpx;
  background: #f4f4f4;
  margin-top: 16rpx;
  max-width: 200rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

#shopCar .goodsItem .goodsInfo .price {
  margin-top: 16rpx;
  color: #ff5850;
}

.title-imgs {
  width: 224rpx;
  height: 166rpx;
  margin: 0 auto;
  margin-top: 50rpx;
}

.title-box {
  text-align: center;
  box-sizing: border-box;
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  margin: 32rpx 0;
}

.to-index-btn {
  margin: 0 auto;
  line-height: 76rpx;
  text-align: center;
  width: 236rpx;
  height: 76rpx;
  background: rgba(250, 190, 0, 1);
  border-radius: 2px;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.submitOrder {
  font-size: 28rpx;
  width: 180rpx;
  height: 76rpx;
  line-height: 76rpx;
  background: rgba(250, 190, 0, 1);
  border-radius: 4rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.submitOrder-gray {
  background-color: #f1f1f1;
  color: gray;
  font-size: 28rpx;
  width: 180rpx;
  height: 76rpx;
  line-height: 76rpx;
  border-radius: 8px;
  font-family: PingFangSC;
  font-weight: 400;
}

.bottomBox {
  box-sizing: border-box;
  padding: 20rpx 20rpx 0 48rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: fixed;
  bottom: 0;
  height: 100rpx;
  width: 750rpx;
  background-color: white;
  z-index: 99;
}

.totalPrice {
  color: #f62e48;
  font-size: 30rpx;
}

.totalPrice-gray {
  color: #a1a1a1;
  font-size: 30rpx;
  text-align: right;
}

.sum {
  position: absolute;
  right: 230rpx;
}

/*主容器*/

.stepper {
  width: 180rpx;
  height: 44rpx;
  /*给主容器设一个边框*/
  border: 1px solid #ededed;
  border-radius: 8rpx;
  vertical-align: middle;
  display: inline-block;
  position: absolute;
  right: 24rpx;
  bottom: 24rpx;
}

/*加号和减号*/

.stepper text {
  width: 50rpx;
  line-height: 44rpx;
  text-align: center;
  float: left;
  box-sizing: border-box;
  color: #999;
}

/*数值*/

.stepper input {
  box-sizing: border-box;
  width: 80rpx;
  height: 44rpx;
  float: left;
  margin: 0 auto;
  text-align: center;
  font-size: 24rpx;
  /*给中间的input设置左右边框即可*/
  border-left: 1px solid #ededed;
  border-right: 1px solid #ededed;
  color: #666;
}

/*禁用样式*/

.stepper .disabled {
  color: #999;
  background: rgba(0, 0, 0, 0.1);
}

button::after {
  border: none;
}

.like {
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: rgba(250, 190, 0, 1);
  height: 44rpx;
  line-height: 44rpx;
  margin: 30rpx 0;
  margin-top: 50rpx;
}

.like view {
  display: inline-block;
  vertical-align: middle;
}

.like .text {
  margin: 0 20rpx;
}

.like .line {
  width: 40rpx;
  height: 2rpx;
  background: #fabe00;
}

.goodsList-like {
  width: 740rpx;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  flex-wrap: wrap;
}

.goodsList-like .goods-item {
  width: 96%;
  height: 530rpx;
  box-sizing: border-box;
  position: relative;
  background-color: #fff;
  margin-left: auto;
  margin-right: auto;
}

.goodsList-like .goods-item .imgBox {
  width: 100%;
  height: 335rpx;
  margin-left: auto;
  margin-right: auto;
  overflow: hidden;
  margin-bottom: 10rpx;
}

.goodsList-like .goods-item .imgBox .goodsImg {
  width: 100%;
  height: 335rpx;
  vertical-align: middle;
}

.goodsList-like .goods-item .goodsName {
  height: 74rpx;
  font-size: 28rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(34, 34, 34, 1);
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  padding: 15rpx 20rpx 0 20rpx;
  text-align: left;
}

.goodsList-like .goods-item .goodsName .select {
  vertical-align: middle;
  display: inline-block;
  width: 64rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  margin-right: 10rpx;
  margin-top: -5rpx;
  background: rgba(255, 88, 80, 1);
  border-radius: 0px 2px 0px 2px;
  font-size: 11px;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
}

.goodsList-like .goods-item .goodsName .text {
  display: inline-block;
  width: 275rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.goodsList-like .goods-item:nth-of-type(even) {
  border-right: none;
}

.goodsList-like .goods-item .goodsPrice {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 600;
  color: rgba(51, 51, 51, 1);
  display: inline-block;
}

.price-bar {
  width: 90%;
  position: absolute;
  bottom: 36rpx;
  height: 30rpx;
  line-height: 30rpx;
  left: 0;
  right: 0;
  margin: 0 auto;
  text-align: left;
}

.total-sale {
  font-size: 22rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  float: right;
}

.radio_chek {
  vertical-align: middle;
  width: 32rpx;
  height: 32rpx;
}

.total-amount {
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.openCardDialog {
  height: 44rpx;
  line-height: 108rpx;
  display: flex;
  justify-content: center;
  position: relative;
  margin: 40rpx 24rpx 0;
}

.openCardDialogTitle {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
}

.closeCardDialog {
  width: 50rpx;
  height: 50rpx;
  color: #999;
  position: absolute;
  top: 28rpx;
  right: 28rpx;
}

.openCardFormItem {
  display: flex;
  justify-content: space-between;
  margin: 24rpx 24rpx;
}

.openCardFormItem-label {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.openCardFormItem-value {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(245, 35, 64, 1);
  line-height: 40rpx;
}

.openCardFormItem-tip {
  text-align: right;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
  display: block;
  margin: 8rpx 24rpx;
}

.openCardFormItem-note {
  margin: 32rpx 24rpx;
  display: block;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 34rpx;
}

.dialogBox {
  width: 750rpx;
  height: 790rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  z-index: 3;
}

.openCardFormSubmit {
  width: 702rpx;
  height: 88rpx;
  position: fixed;
  bottom: 32rpx;
  left: 24rpx;
  background: rgba(250, 190, 0, 1);
  border-radius: 8rpx;
}

.enjoyment-openedCard-mark {
  width: 32rpx;
  height: 32rpx;
  vertical-align: middle;
  overflow: hidden;
  margin-left: 8rpx;
}

.sum-saveMoney-pic {
  width: 26rpx;
  height: 26rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 88, 80, 1);
  line-height: 26rpx;
  vertical-align: middle;
  margin-left: 4rpx;
}

.sum-saveMoney-txt {
  height: 26rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 88, 80, 1);
  line-height: 26rpx;
}

.sum-saveMoney-box{
  vertical-align: baseline;
}

.mask {
  width: 750rpx;
  height: 100%;
  position: fixed;
  background: #000;
  opacity: 0.3;
  top: 0;
  z-index: 2;
}