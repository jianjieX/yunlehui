.nav {
  width: 100%;
  height: 92rpx;
  line-height: 92rpx;
  display: flex;
  box-sizing: border-box;
  border-top: 1px solid #f1f1f1;
  /* border-bottom: 1px solid #f1f1f1; */
  background: #fff;
  align-items: center;
  font-size: 28rpx;
  color: #999999;
}


.nav view {
  flex: 1;
  height: 100%;
  text-align: center;
}

.focus {
  color:#333333;
  font-weight:500;
  position: relative;
}

.focus::after{
  display: block;
  content:'';
  position: absolute;
  width: 32rpx;
  height:4rpx;
  background:#FABE00;
  left:50%; 
  transform: translate(-50%);
  z-index:999;
  bottom:0;
}