  height: 88rpx;
  /* width: 100%; */
  margin-left: 54rpx;
  display: flex;
  align-items: center;
}

.user-icon {
  width: 88rpx;
  height: 88rpx;
  border-radius: 50%;
}

.user-content {
  margin-left: 16rpx;
}

.user-name {
  height: 44rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(141, 72, 10, 1);
