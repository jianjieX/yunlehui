/* pages/enjoyCard/enjoyCard.wxss */
@import "../../wxParse/wxParse.wxss";

page {
  background: #ffffffff;
}

/* 取消默认滚动条 */

::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/* 乐享卡 */

.enjoy-card-container {
  padding-top: 32rpx;
  display: flex;
  width: 100%;
  justify-content: center;
}

.enjoy-card-background {
  width: 702rpx;
  height: 400rpx;
}

.enjoy-card {
  position: absolute;
  box-sizing: border-box;
  padding: 0 14rpx 56rpx 56rpx;
  width: 702rpx;
  height: 400rpx;
}

.enjoy-title {
  margin: 66rpx 0rpx 56rpx 0rpx;
  height: 60rpx;
  font-size: 44rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(94, 45, 1, 1);
  line-height: 60rpx;
}

.enjoy-font-color {
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #8d480a;
}

.enjoy-text {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(141, 72, 10, 1);
  line-height: 34rpx;
}

.open-card {
  display: flex;
  justify-content: space-between;
}

.price-enjoy {
  font-size: 40rpx;
  margin-top: 8rpx;
}

.open-card-botton {
  position: relative;
  top: -16rpx;
  width: 288rpx;
  height: 124rpx;
}

.open-card-img {
  width: 288rpx;
  height: 124rpx;
}

.openCard {
  margin: 70rpx auto 0 auto;
  line-height: 80rpx;
  text-align: center;
  width: 550rpx;
  height: 80rpx;
  background: rgba(250, 190, 0, 1);
  border-radius: 8rpx;
}

.opencard-text {
  top: 1rpx;
  left: 25rpx;
  position: absolute;
  width: 236rpx;
  height: 108rpx;
  line-height: 108rpx;
  text-align: center;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(141, 72, 10, 1);
}

.openCardAuthority {
  background: rgba(250, 190, 0, 1);
  border-radius: 4px;
  text-align: center;
  line-height: 80rpx;
  width: 550rpx;
  height: 80rpx;
  position: fixed;
  bottom: 36rpx;
  left: 50rpx;
}

/* 六大特权 */

.six-vip-authority {
  height: 50rpx;
  margin-top: 12rpx;
  display: flex;
  padding: 0rpx 32rpx;
  justify-content: space-between;
  align-items: cneter;
  margin-bottom: 40rpx;
}

.authority-title {
  font-weight: 500;
  font-size: 36rpx;
  color: rgba(51, 51, 51, 1);
}

.authority-discrip {
  font-size: 12px;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.left, .right {
  width: 342rpx;
}

.left {
  float: left;
  margin-left: 24rpx;
}

.right {
  margin-right: 24rpx;
  float: right;
}

.mask {
  background: black;
  z-index: 1;
  position: fixed;
  width: 100%;
 
  opacity: 0.6;
}
.mask-up {
  z-index: 2;
  position: fixed;
  width: 100%;
 
}

.mask-showAuthority {
  z-index: 9999;
  position: absolute;
  width: 632rpx;
  height: 796rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 8rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -60%);
}



/* 模态框x */

.cancelMask {
  box-sizing: border-box;
  height: 56rpx;
  width: 100%;
  padding-top: 24rpx;
  padding-right: 28rpx;
  display: flex;
  justify-content: space-between;
}

.cancelMask image {
  width: 32rpx;
  height: 32rpx;
}

.authorityTitle {
  text-align: center;
  font-size: 36rpx;
  height: 50rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 550;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
  margin-bottom: 48rpx;
}

/* scroll */

.scroll-container {
  width: 750rpx;
  white-space: nowrap;
}

.scroll-items-container {
  display: inline-block;
  width: 160rpx;
  height: 160rpx;
}

.scroll-items {
  display: flex;
  flex-direction: column;
  align-items: center;
}

.scroll-items image {
  width: 76rpx;
  height: 76rpx;
}

.authority-item-title {
  margin-top: 14rpx;
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.authority-item-desc {
  height: 34rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

/* 开卡好礼 */

.open-card-text {
  padding-left: 32rpx;
  margin-top: 50rpx;
  margin-bottom: 32rpx;
}

.open-card-heard {
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
}

.open-card-body {
  margin-top: 8rpx;
  height: 32rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 32rpx;
}

/* 开卡礼包 */

.product-image-container {
  position: relative;
  width: 342rpx;
  height: 342rpx;
  background: #fff;
}

.product-image {
  width: 342rpx;
  height: 342rpx;
}

.free-get-image {
  bottom: 0;
  left: 0;
  position: absolute;
  width: 342rpx;
  height: 74rpx;
}

.content {
  width: 100%;
  overflow: hidden;
}

.product-item {
  padding-bottom: 24rpx;
  width: 100%;
  margin-bottom: 24rpx;
  box-shadow: 0px 4rpx 8rpx 0rpx rgba(0, 0, 0, 0.1);
}

/* 乐享专区 */

.enjoy-self-text {
  margin: 48rpx 0rpx 24rpx 30rpx;
  height: 50rpx;
  font-size: 36rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 50rpx;
}

/* 商品模板 */

.product-type {
  margin-top: -5rpx;
  display: inline-block;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  text-align: center;
  font-size: 22rpx;
  line-height: 32rpx;
  width: 64rpx;
  height: 32rpx;
  background: rgba(255, 88, 80, 1);
  border-radius: 0px 2px 0px 2px;
  vertical-align: middle;
  margin-right: 4rpx;
}

.product-item .product-title {
  height: 72rpx;
  margin: 16rpx 8rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(34, 34, 34, 1);
  white-space: normal;
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}

.enjoy-self-container {
  overflow: hidden;
}

.enjoy-price {
  margin-left: 5rpx;
  display: flex;
  height: 32rpx;
  align-items: center;
  font-size: 24rpx;
}

.enjoy-price-text {
  font-size: 24rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(245, 35, 64, 1);
}

.enjoy-price-now {
  margin: 0rpx 8rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(245, 35, 64, 1);
}

.enjoy-price-dis {
  padding: 0 8rpx;
  background: rgba(245, 35, 64, 0.08);
  color: rgba(245, 35, 64, 1);
  border-radius: 4rpx;
  margin: 0 4rpx;
}

.enjoy-price-disText {
  height: 26rpx;
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 26rpx;
}

.enjoy-price-disText1 {
  height: 26rpx;
  font-size: 16rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #F73953;
  line-height: 26rpx;
}

.enjoy-price-old {
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-decoration: line-through;
  margin-left: 4rpx;
}

/* 底部占位 */

.footer-empty {
  width: 100%;
  height: 136rpx;
}

/* 底部按钮 */

.footer {
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 136rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #ffffff;
}

.footer-bottom {
  text-align: center;
  line-height: 88rpx;
  width: 702rpx;
  height: 88rpx;
  background: rgba(250, 190, 0, 1);
  border-radius: 4px;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  font-size: 36rpx;
}

/* 富文本css */

.desc-title {
  text-align: center;
  width: 504rpx;
  height: 48rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 48rpx;
}


.authorityDesc .desc-image1 {
  margin: 56rpx 134rpx 0 134rpx;
  width: 364rpx;
  height: 308rpx;
}

.authorityDesc .desc-image2 {
  margin: 28rpx 178rpx 0 178rpx;
  width: 276rpx;
  height: 276rpx;
}

.authorityDesc .desc-image3 {
  margin: 64rpx 140rpx 0 130rpx;
  width: 362rpx;
  height: 234rpx;
}
.authorityDesc .desc-image4 {
  margin: 22rpx 160rpx 0 160rpx;
  width: 314rpx;
  height: 210rpx;
}

/* 有卡 */

.have-enjoy-card {
  position: absolute;
  width: 702rpx;
  height: 400rpx;
}

.enjoyCardId {
  margin-left: 30rpx;
  margin-top: 8rpx;
  height: 44rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Medium, PingFang-SC;
  font-weight: 500;
  color: rgba(196, 167, 110, 1);
