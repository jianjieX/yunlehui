}

.deductionFormItem-left-text {
  width: 110rpx;
  height: 32rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(183, 117, 58, 1);
  line-height: 32rpx;
  text-align: center;
}

.deductionFormItem-middle {
  margin: 36rpx 56rpx 35rpx 0;
