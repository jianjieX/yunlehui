.dialog {
  width: 750rpx;
  height: 790rpx;
  background: rgba(255, 255, 255, 1);
  position: fixed;
  bottom: 0;
  z-index: 999;
  left: 0;
}

.dialog .closeDislog {
  width: 100%;
  height: 40rpx;
  padding: 24rpx 24rpx 0 0;
  box-sizing: border-box;
  display: flex;
  justify-content: flex-end;
}

.dialog .closeDislog .closeDislogIcon {
  width: 22rpx;
  height: 22rpx;
  font-size: 22rpx;
  color: #999999;
}

.deductionDialogTitle {
  width: 750rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  text-align: center;
  margin-bottom: 24rpx;
}

.folding-view-status {
  display: inline-block;
  height: 14rpx;
  width: 14rpx;
  border-width: 4rpx 0rpx 0rpx 4rpx;
  margin-top: -7rpx;
  border-color: #000;
  border-style: solid;
  -webkit-transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
  transform: matrix(0.71, 0.71, -.71, 0.71, 0, 0);
}

.deduction-total {
  height: 40rpx;
  padding: 0 24rpx;
  display: flex;
  justify-content: space-between;
}

.deduction-total-left {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
