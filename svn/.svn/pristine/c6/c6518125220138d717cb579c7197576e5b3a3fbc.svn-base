  line-height: 100rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(141, 72, 10, 1);
}

.deductionFormItem-left-money-value {
  display: flex;
  flex-direction: row;
  justify-content: center;
  font-size: 56rpx;
  line-height: 80rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  text-align: center;
  font-weight: 500;
  color: rgba(141, 72, 10, 1);
