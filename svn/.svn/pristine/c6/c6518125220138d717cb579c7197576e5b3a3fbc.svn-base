}

.deductionFormItem-right {
  margin-top: 34rpx;
  display: inline-block;
}

.deductionFormItem-right-img {
  position: absolute;
  width: 42rpx;
  height: 42rpx;
  top: 74rpx;
  right: 34rpx;
}

.deductionForm-button {
  width: 702rpx;
  height: 88rpx;
  position: fixed;
  bottom: 32rpx;
  left: 24rpx;
  background: rgba(250, 190, 0, 1);
  border-radius: 8rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
}

.deductionForm-button .title {
  text-align: center;
  font-size: 36rpx;
  margin-left: 154rpx;
}

.deductionForm-button .detail {
  min-width: 154rpx;
  height: 88rpx;
  font-size: 24rpx;
  margin-top: 6rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 88rpx;
}

.list-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 40;
  /*backdrop-filter: blur(10px);*/
  -webkit-backdrop-filter: 1;
  background: rgba(0, 0, 0, 0.3);
}

.list-mask :fade-enter-active, :fade-leave-active {
  opacity: 1;
  transition: all 0.5s;
  background: rgba(7, 17, 27, 0.6);
}
