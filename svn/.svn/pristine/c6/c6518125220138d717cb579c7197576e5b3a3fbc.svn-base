}

.deduction-total-right {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.deduction-total-right-text {
  width: 112rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 88, 80, 1);
  line-height: 40rpx;
}

.folding-view-big {
  height: 40rpx;
  text-align: right;
  line-height: 40rpx;
  display: inline-block;
}

.shouqi {
  font-size: 28rpx;
  color: #c7c7cc;
  margin-left: 16rpx;
}

.deductionFormItem {
