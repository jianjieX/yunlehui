  flex-direction: column;
}

.dedutionFormItem-middle-title {
  width: 300rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
