.order-wapper {
  border-top: 1rpx solid #dcdcdc;
  position: relative;
  width: 100%;
  padding: 24rpx;
  box-sizing: border-box;
  height: auto;
  margin-bottom: 88rpx;
}

.order-wapper .mode {
  width: 100%;
  height: 88rpx;
  background-color: #fff;
  border-radius: 8rpx;
  margin-bottom: 16rpx;
  padding: 24rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: row;
}

.order-wapper .mode .mode-title {
  width: 116rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.order-wapper .mode .mode-check .radio-group {
  margin-top: -2rpx;
  line-height: 40rpx;
  margin-left: 80rpx;
}

.order-wapper .mode .mode-check .radio-group .radio {
  line-height: 24rpx;
  transform: scale(0.7);
  margin-left: 56rpx;
}

.order-wapper .mode .mode-check .radio-group .radio .text {
  margin-left: 10rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  font-size: 40rpx;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
}

.order-wapper .content {
  width: 100%;
  min-height: 320rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 24rpx;
  box-sizing: border-box;
  margin-bottom: 16rpx;
}

.order-wapper .content .shopcard {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  flex-direction: row;
}

.order-wapper .content .shopcard .shop {
  font-size: 28rpx;
  line-height: 40rpx;
}

.order-wapper .content .shopcard .shop-name {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  margin-left: 18rpx;
  margin-top: -2rpx;
  line-height: 40rpx;
}

.order-wapper .content .goods {
  display: flex;
  flex-direction: column;
  border-bottom: 2rpx solid #f5f5f5;
}

.order-wapper .content .goods .detail {
  margin: 30rpx 0 30rpx 0;
  width: 100%;
  height: 104rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.order-wapper .content .goods .detail .foods {
  display: flex;
  flex-direction: row;
}

.order-wapper .content .goods .detail .foods .image {
  width: 104rpx;
  height: 104rpx;
  border-radius: 4rpx;
}

.order-wapper .content .goods .detail .foods .name {
  display: flex;
  flex-direction: column;
  margin-left: 24rpx;
}

.order-wapper .content .goods .detail .foods .name .title {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.order-wapper .content .goods .detail .foods .name .number {
  margin-top: 4rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
}

.order-wapper .content .goods .detail .foods .name .spec {
  margin-top: 4rpx;
  height: 16px;
  font-size: 12px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 17px;
}

.order-wapper .content .goods .detail .price {
  display: flex;
  flex-direction: column;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
}

.order-wapper .content .goods .detail .price .benefitPrice {
  text-align: right;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  text-decoration: line-through;
}

.order-wapper .content .sum {
  width: 100%;
  height: 44rpx;
  margin-top: 20rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.order-wapper .content .sum .title {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
  margin-right: 32rpx;
}

.order-wapper .content .sum .total {
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 44rpx;
}

input {
  height: 104rpx;
  line-height: 104rpx;
  text-align: right;
  color: #222;
}

.order-wapper .benefit {
  margin-top: 16rpx;
  width: 100%;
  height: 92rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 0 24rpx 0 24rpx;
  box-sizing: border-box;
}

.order-wapper .benefit .item {
  width: 100%;
  height: 92rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 82rpx;
}

.order-wapper .benefit .item .title {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 92rpx;
}

.order-wapper .benefit .item .title .arrow {
  width: 16rpx;
  height: 16rpx;
  border-bottom: 2rpx solid #666;
  border-left: 2rpx solid #666;
  display: inline-block;
  transform: rotate(225deg);
  margin-bottom: 0rpx;
  margin-left: 2px;
}

.order-wapper .contact {
  margin-top: 16rpx;
  width: 100%;
  height: 164rpx;
  background-color: #fff;
  border-radius: 8rpx;
  padding: 0 24rpx 0 24rpx;
  box-sizing: border-box;
}

.order-wapper .contact .person, .mobile {
  width: 100%;
  height: 82rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  line-height: 82rpx;
}

.order-wapper .contact  .person:first-of-type {
  border-bottom: 2rpx solid #f5f5f5;
}

.order-wapper .contact .person .title, .mobile .title {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 82rpx;
}

.order-wapper .contact  .person .name, .mobile .num {
  height: 82rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  line-height: 82rpx;
}

.settlement {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  z-index: 50;
  width: 100%;
  height: 88rpx;
  display: flex;
  flex-direction: row;
}

.settlement .content-left {
  width: 558rpx;
  height: 88rpx;
  background-color: #000;
  line-height: 88rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-end;
}

.settlement .content-left .total {
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  margin-right: 16rpx;
  color: rgba(255, 255, 255, 1);
}

.settlement .content-left .price {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin-right: 48rpx;
}

.settlement .content-right {
  flex: 1;
  height: 88rpx;
  border: 0rpx;
  background-color: #fabe00;
  border-radius: 0rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  text-align: center;
  line-height: 88rpx;
}

button[disabled] {
  background-color: #fabe00 !important;
  color: rgba(51, 51, 51, 1) !important;
  border: 0rpx !important;
  border-radius: 0rpx !important;
}

button::after {
  border: 0rpx solid #fabe00;
  border-radius: 0rpx;
}

.pay-mask {
  width: 100%;
  height: 100%;
  background: #000;
  opacity: 0.5;
  z-index: 9000;
  position: fixed;
  top: 0;
  left: 0;
}

