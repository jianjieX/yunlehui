}

.undeductionFormItem {
  height: 188rpx;
  margin: 32rpx 24rpx;
  background: #dcdcdc;
  border-radius: 8rpx;
  border: 2rpx solid #d3d3d3;
  box-sizing: border-box;
  display: flex;
  position: relative;
}

.list-content {
  max-height: 490rpx;
  overflow: hidden;
  background: #fff;
  box-sizing: border-box;
}

.deductionFormItem-left {
  width: 110rpx;
  height: 112rpx;
  margin: 34rpx 56rpx 42rpx 54rpx;
