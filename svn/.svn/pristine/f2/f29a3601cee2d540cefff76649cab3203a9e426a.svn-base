page {
  background: #fff;
  width: 100%;
}

.header {
  height: 400rpx;
  position: relative;
}

.header .bg {
  width: 100%;
  height: 100%;
}

.header .level {
  width: 240rpx;
  height: 220rpx;
  position: absolute;
  top: 60rpx;
  left: 50%;
  transform: translateX(-50%);
}

.header .level image {
  width: 100%;
  height: 100%;
}

.header .level .level-num {
  position: absolute;
  left: 50%;
  top: 30%;
  transform: translate(-50%);
  color: #fff;
  font-size: 40rpx;
}

.header .level .tip {
  font-size: 18rpx;
  position: absolute;
  color: #fff;
  bottom: 30rpx;
  left: 40rpx;
}

.header .level-bar {
  width: 450rpx;
  height: 100rpx;
  position: absolute;
  bottom: 30rpx;
  left: 50%;
  transform: translateX(-50%);
}

.header .level-bar progress {
  margin-top: 40rpx;
}

.header .level-item {
  position: absolute;
  z-index: 9;
  left: -20rpx;
  top: 16rpx;
}

.header .level-item.v_1 {
  position: absolute;
  left: 200rpx;
  top: 16rpx;
}

.header .level-item.v_2 {
  position: absolute;
  left: 420rpx;
  top: 16rpx;
}

.header .level-item-num {
  color: #fff;
  position: absolute;
  font-size: 24rpx;
  left: 16rpx;
  top: 12rpx;
}

.header .level-item image {
  width: 60rpx;
  height: 60rpx;
}

.header .growth {
  position: absolute;
  left: -9999rpx;
  top: 20rpx;
}

.header .growth image {
  width: 22rpx;
  height: 22rpx;
}

.header .growth .growthNum {
  color: #fadc7d;
  font-size: 24rpx;
  text-align: center;
}

.content {
  padding: 0 20rpx;
  margin-bottom: 50rpx;
}

.member-level {
  width: 100%;
  height: 200rpx;
}

.bgimg-v0 {
  width: 100%;
  height: 200rpx;
}

.member-level-item {
}

.slide-image {
  width: 320rpx;
  height: 200rpx;
}

.swipe-item {
  position: relative;
}

.swipe-item .text {
  position: absolute;
  left: 50rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size: 24rpx;
  color: #a6790c;
}

.swipe-item .text view {
  margin: 10rpx 0;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}

.table {
  background: #fdf8ea;
  border-radius: 8rpx 8rpx 0 0;
  overflow: hidden;
}

.table .tr {
  display: flex;
}

.table .table-head {
  background: linear-gradient(339deg, rgba(254, 226, 96, 1) 0%, rgba(252, 194, 49, 1) 100%);
}

.table .table-head .td {
  color: #fff;
  padding: 16rpx 0;
  height: 120rpx;
  line-height: 120rpx;
}

.table .td {
  /* flex: 1; */
  border: 1px solid #fff;
  text-align: center;
  padding: 20rpx 0;
}

.table .col_1 {
  width: 140rpx;
  color: #a6790c;
}

.table .col_2 {
  width: 200rpx;
  color: #fec637;
}

.table .col_3 {
  flex: 1;
  color: #fe746b;
}

.growth-value {
  border: 1px solid #d6d6d6;
  background: #fcfbff;
  border-radius: 12rpx;
  overflow: hidden;
}

.growth-value .tr {
  display: flex;
}

.growth-value .td {
  border: 1px solid #f1f1f2;
  height: 100rpx;
  line-height: 100rpx;
  box-sizing: border-box;
  padding: 0 20rpx;
}

.growth-value .td_title {
  flex: 1;
  color: #a6790c;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
}

.growth-value .td_value {
  width: 140rpx;
  text-align: center;
  color: #fe5a53;
}

.content .title {
  height: 100rpx;
  margin: 26rpx auto;
  width: 300rpx;
  display: flex;
  align-items: center;
}

.content .title image {
  width: 100%;
  height: 100%;
}

.content .update {
  width: 100%;
  margin: 0 auto;
  position: relative;
  background: linear-gradient(315deg, rgba(251, 230, 145, 1) 0%, rgba(246, 201, 88, 1) 100%);
  border-radius: 8rpx;
  padding: 40rpx 0;
}

.up-level-flex {
  width: 600rpx;
  display: flex;
  flex-flow: row;
  margin: 12rpx auto;
}

.up-level-point {
  width: 16rpx;
  height: 16rpx;
  background: rgba(232, 178, 47, 1);
  border-radius: 50%;
  margin-top: 12rpx;
}

.up-level-text {
  font-size: 28rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(166, 121, 12, 1);
  margin-left: 16rpx;
}

.content .child-title {
  text-align: center;
  margin: 30rpx 0;
}

.content .child-title text {
  position: relative;
}

.content .child-title text::after {
  content: "";
  width: 100%;
  position: absolute;
  height: 6rpx;
  background: #fadc7d;
  left: 0;
  bottom: -10rpx;
}

.content .goods-list {
  display: flex;
  flex-wrap: wrap;
  width: 700rpx;
}

.content .goods-list .goods-item {
  width: 310rpx;
  height: 540rpx;
  border: 1px solid #f1f1f1;
  box-sizing: border-box;
  margin: 20rpx;
  border-radius: 10rpx;
}

.content .goods-list .goods-item .img {
  width: 100%;
  height: 310rpx;
}

.content .goods-list .goods-item image {
  width: 100%;
  height: 310rpx;
}

.content .goods-list .goods-item .goods-name {
  padding: 0 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.content .goods-list .goods-item .price {
  padding: 20rpx 10rpx;
  color: #ff5850;
  text-align: center;
}

.content .goods-list .goods-item .buy {
  margin-left: auto;
  margin-right: auto;
  text-align: center;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 60rpx;
  border: 1px solid #f4bb2e;
  color: #f4bb2e;
  font-size: 26rpx;
}

.goTop {
  height: 65rpx;
  width: 65rpx;
  position: fixed;
  bottom: 50rpx;
  background: rgba(0, 0, 0, 0.3);
  right: 30rpx;
  border-radius: 50%;
  font-size: 24rpx;
  text-align: center;
  line-height: 34rpx;
  padding: 10rpx;
  color: white;
}
