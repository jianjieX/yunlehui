page {
  background: #f0f0f0;
}

/* user背景图 */

.user-background {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  right: 30rpx;
  z-index: 999;
}

.top-back-view {
  background: #fff;
  width: 100%;
  position: absolute;
  top: 0;
  z-index: 999;
}

/* 消息 */

.option-view {
  width: 34rpx;
  height: 28rpx;
  z-index: 999;
  position: absolute;
  left: 34rpx;
}

.option-img {
  width: 36rpx;
  height: 36rpx;
  z-index: 999;
}

.red-point {
  width: 20rpx;
  height: 20rpx;
  background: rgba(255, 88, 80, 1);
  z-index: 999;
  position: absolute;
  left: 50rpx;
  border-radius: 50%;
}

.padding-income-view {
  width: 702rpx;
  height: 24rpx;
  background: #fff;
  margin: 24rpx auto 0rpx auto;
}

/* 用户显示区 */

.avatar-view {
  width: 112rpx;
  height: 112rpx;
  border: 1px solid rgba(255, 255, 255, 1);
  position: absolute;
  border-radius: 50%;
  left: 24rpx;
  z-index: 999;
}

.user-name-view {
  position: absolute;
  left: 160rpx;
  z-index: 999;
}

.user-name-style {
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  /* line-height: 36rpx; */
}

.user-point {
  height: 16rpx;
  width: 16rpx;
  border-top: 2rpx solid #666;
  border-right: 2rpx solid #666;
  transform: rotate(45deg) translateY(14rpx);
}

.name-and-ranking {
  width: 540rpx;
  display: flex;
  justify-content: space-between;
}

.ranking-img {
  width: 28rpx;
  height: 36rpx;
  margin: auto 0 auto 15rpx;
}

.vip-level-style {
  width: 106rpx;
  height: 40rpx;
  line-height: 40rpx;
  background: linear-gradient(308deg, rgba(221, 140, 27, 1) 0%, rgba(254, 199, 92, 1) 100%);
  border-radius: 30rpx;
  font-size: 26rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  text-align: center;
}

.personal-profile {
  height: 20px;
  position: absolute;
  left: 160rpx;
  z-index: 999;
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  line-height: 22px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  width: 440rpx;
  margin-top: 4rpx;
}

/* 我的会员权益 */

.my-vip {
  background: #c6c6c6;
  display: flex;
  padding: 0 30rpx;
  height: 80rpx;
  justify-content: space-between;
  align-items: center;
  border-radius: 10rpx 10rpx 0 0;
  margin: 0rpx 32rpx;
}

.my-vip .my-vip-left {
  font-size: 28rpx;
}

.my-vip .my-vip-right {
  background: #ededed;
  padding: 0 20rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  border-radius: 10rpx;
}

/* 乐享卡 */

.enjoyCard {
  width: 700rpx;
  padding: 10rpx 25rpx 0rpx 25rpx;
  height: 80rpx;
  background: #fff;
  position: relative;
}

.enjoyCard image {
  width: 100%;
  height: 80rpx;
}

.enjoyCard-botton {
  box-sizing: border-box;
  padding: 0 30rpx 0 36rpx;
  width: 700rpx;
  height: 80rpx;
  position: absolute;
  top: 10rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.enjoyCard-botton-text {
  height: 28rpx;
  font-size: 24rpx;
  font-family: FZHTJW--GB1-0, FZHTJW--GB1;
  font-weight: normal;
  color: rgba(51, 51, 51, 1);
  line-height: 28rpx;
}

.openNow {
  display: flex;
  width: 162rpx;
  height: 32rpx;
  padding-left: 10rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 1px 2px 0px rgba(0, 0, 0, 0.1);
  border-radius: 26rpx;
  opacity: 0.89;
  align-items: center;
}

.openNow-text {
  text-align: center;
  width: 120rpx;
  margin-left: 8rpx;
  margin-right: 4rpx;
  height: 32rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 32rpx;
}

.arrow {
  width: 0;
  height: 0;
  border: 6rpx rgba(255, 255, 255, 0) solid;
  border-left: 6rpx rgba(51, 51, 51, 1) solid;
}

/**我的收益**/

.pl-view {
  width: 100%;
  height: 36px;
  background: linear-gradient(143deg, #fcd400 0%, rgba(250, 190, 0, 1) 100%);
  position: absolute;
}

.my-income-view {
  height: 120rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
  display: flex;
  z-index: 666;
  margin: 0rpx 25rpx 16rpx 25rpx;
}

.item-income-view {
  display: flex;
  flex: 1;
  flex-direction: column;
  text-align: center;
}

.dividing-line {
  width: 2rpx;
  height: 48rpx;
  margin: auto 0;
  background-color: #666;
}

.income-money-style {
  height: 52rpx;
  line-height: 56rpx;
  font-size: 40rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  /* margin-top: 24rpx; */
}

.income-type-style {
  font-size: 24rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin-top: 12rpx;
}

.rmb-style {
  font-size: 24rpx;
}

/**我的订单**/

.my-order-view {
  width: 700rpx;
  height: 240rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
  margin: 16rpx auto 0 auto;
  z-index: 666;
  position: relative;
}

.my-order-see-all-order {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
  border-bottom: 1px solid #ededed;
}

.my-order-title {
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  position: absolute;
  left: 24rpx;
}

.see-all-order {
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 500;
  color: rgba(153, 153, 153, 1);
  position: absolute;
  right: 24rpx;
}

.order-type-view {
  width: 100%;
  display: flex;
  flex-flow: row;
}

.order-type-item {
  flex: 1;
  height: 160rpx;
  align-items: center;
  display: flex;
  flex-direction: column;
}

.order-type-image-view {
  display: flex;
  margin-left: auto;
  margin-right: auto;
  margin-top: 34rpx;
  position: relative;
}

.order-type-num {
  background: rgba(249, 82, 81, 1);
  border-radius: 14rpx;
  position: absolute;
  right: -18rpx;
  top: -13rpx;
  font-size: 18rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  padding: 0rpx 10rpx;
}

.order-type-image {
  width: 48rpx;
  height: 48rpx;
}

.order-type-text {
  color: rgba(51, 51, 51, 1);
  font-size: 24rpx;
  font-family: PingFang-SC-Medium;
  font-weight: 400;
  margin-top: 24rpx;
}

/* 我的活动 */

.activity {
  display: flex;
  height: 148rpx;
  background: #fff;
  margin: 16rpx 24rpx;
}

.activity .activity-item {
  color: rgba(51, 51, 51, 1);
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: center;
  width: 152rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
}

/* 渐变条 */

.activity-line {
  margin-top: 30rpx;
  width: 16rpx;
  height: 88rpx;
}

.activity .activity-item image {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 16rpx;
}

/* 所有的活动状态 */

.activity .activity-allState {
  margin-top: 30rpx;
  width: 446rpx;
  display: flex;
  margin-left: 56rpx;
  justify-content: space-between;
  margin-bottom: 20rpx;
}

.activity .activity-state {
  font-size: 24rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.activity .activity-state text {
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

/* 活动状态图片 */

.activity .activity-state image {
  width: 48rpx;
  height: 48rpx;
}

/* 我的服务 */

.my-serve {
  background: #fff;
  margin: 0 24rpx;
  display: flex;
  height: 148rpx;
}

.my-serve .my-serve-item image {
  margin-top: 28rpx;
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 8rpx;
}

.my-serve .my-serve-item {
  display: flex;
  flex-direction: column;
  width: 152rpx;
  align-items: center;
  /* padding: 38rpx 0 16rpx 0; *//* justify-content: space-between; */
}

/* 我的服务概括 */

.my-serve .my-serve-item text {
  color: rgba(51, 51, 51, 1);
  height: 36rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  line-height: 36rpx;
}

/* 我的服务所有状态 */

.my-serve .my-serve-allState {
  margin-left: 56rpx;
  padding: 34rpx 0 16rpx 0rpx;
  display: flex;
  justify-content: space-between;
}

.my-serve .my-serve-allState text {
  color: rgba(51, 51, 51, 1);
  height: 36rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
}

.my-serve .my-serve-state {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}

.my-serve .my-serve-state image {
  width: 48rpx;
  height: 48rpx;
}

.my-serve .my-serve-state:last-of-type {
  margin-left: 116rpx;
}

/**必备工具**/

.my-utils-view {
  width: 700rpx;
  background: rgba(255, 255, 255, 1);
  border-radius: 4rpx;
  margin: 16rpx auto 0 auto;
  display: flex;
  flex-wrap: wrap;
}

.my-item-util {
  width: 25%;
  height: 130rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.util-name {
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  margin: 12rpx 0 16rpx 0;

}

.img-open-shop {
  width: 48rpx;
  height: 48rpx;
  margin-top: 20rpx;
}

.goods-wrap {
  padding-bottom: 20rpx;
}

.goods-wrap .goods-header {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  height: 44rpx;
  line-height: 44rpx;
  font-size: 32rpx;
  text-align: center;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(183, 117, 58, 1);
  padding-top: 32rpx;
  padding-bottom: 12rpx;
}

.goods-wrap .goods-header .title {
  width: 224rpx;
  height: 44rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(183, 117, 58, 1);
  line-height: 44rpx;
  margin:0 16rpx;
}

.goods-wrap .goods-header .tag1 {
  width: 15rpx;
  height: 18rpx;
  background: linear-gradient(360deg, rgba(255, 232, 186, 1) 0%, rgba(250, 151, 0, 1) 100%);
  transform: rotate(40deg);
  border-radius: 5rpx;
  margin: 0 4rpx;
}

.goods-wrap .goods-header .tag2 {
  width: 17rpx;
  height: 21rpx;
  background: linear-gradient(360deg, rgba(255, 232, 186, 1) 0%, rgba(250, 151, 0, 1) 100%);
  transform: rotate(40deg);
  border-radius: 5rpx;
  margin: 0 4rpx;
}

.goods-wrap {
  width: 100%;
}

.goods-wrap, .goods-content {
  background: rgba(240, 240, 240, 1);

}

.goods-wrap .goods-row {
  padding: 0 24rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}

.goods-wrap .goods-row .item {
  display: flex;
  position: relative;
  margin-top: 20rpx;
  flex-direction: column;
  width: 49%;
  background-color: #fff;
}

.goods-row .item .goodsImg {
  width: 100%;
  height: 335rpx;
  vertical-align: middle;
}

.goods-row .item .bottom {
  width: 92%;
  margin: 24rpx auto;
  display: flex;
  flex-direction: row;
  align-items: center;
  height: 40rpx;
  line-height: 40rpx;
  box-sizing: border-box;
  padding: 10rpx;
}

.goods-row .item .bottom .price {
  font-size: 24rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: rgba(245, 35, 64, 1);
  margin-right: 5rpx;
}

.goods-row .item .tag {
  width: 64rpx;
  height: 32rpx;
  display: inline-block;
  background: rgba(255, 88, 80, 1);
  border-radius: 0px 4px 0px 4px;
  font-size: 22rpx;
  text-align: center;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  margin: 0 4rpx;
}


.goods-row .item .name {
  height: 74rpx;
  font-size: 28rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(34, 34, 34, 1);
  word-break: break-all;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
  padding: 15rpx 20rpx 0 20rpx;
}

.goods-row  .bottom  .enjoy-wrap {
  height: 26rpx;
  line-height: 26rpx;
  background: rgba(245, 35, 64, 0.08);
  border-radius: 4rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin: 0 4rpx;
}
.goods-row  .bottom  .enjoy-wrap text {
  font-size: 18rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);

}

.goods-row  .bottom  .enjoy-wrap .price {
  font-size: 16rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #f52340;
}
.privilege-decorate{
  width:  40rpx;
  height: 40rpx;
}
