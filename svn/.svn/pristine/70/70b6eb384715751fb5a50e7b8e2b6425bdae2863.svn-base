  line-height: 34rpx;
  margin-top: 8rpx;
}

.icon1 {
  width: 44rpx;
  height: 40rpx;
  position: absolute;
  left: 124rpx;
  vertical-align: middle;
}

.icon2 {
  width: 44rpx;
  height: 54rpx;
  position: absolute;
  right: 28rpx;
  vertical-align: middle;
  top: -10rpx;
}


.d-item:last-child .d-item-title {
  display: flex;
  justify-content: center;
  align-items: center;
}

.title-icon {
  width: 32rpx;
  height: 4rpx;
  background: #336ee4;
  margin: 0 16rpx;
}