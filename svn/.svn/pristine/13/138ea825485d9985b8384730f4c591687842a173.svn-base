.shop-card {
  position: fixed;
  border-radius: 8rpx;
  background-color: #fff;
  width: 702rpx;
  height: 192rpx;
  top: 62rpx;
  left: 24rpx;
  z-index: 10;
  box-shadow: 0rpx 4rpx 20rpx 0rpx #d3d3d3;
}

.shop-card .wapper {
  width: 100%;
  padding: 24rpx;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
}

.shop-card .wapper .shop-logo {
  width: 144rpx;
  height: 144rpx;
}

.shop-card .wapper .wapper-right {
  flex: 1;
  display: flex;
  flex-direction: column;
  margin-left: 16rpx;
}

.shop-card .wapper .wapper-right .title {
  margin-top: 24rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.shop-card .wapper .wapper-right .desc {
  width: 100%;
  margin-top: 24rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 32rpx;
}

/* tab */

.tab {
  top: 120rpx;
  display: flex;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
}

.tab .tab-item {
  width: 60rpx;
  height: 70rpx;
  margin-left: 228rpx;
  text-align: center;
  font-size: 28rpx;
}

/* .tab .tab-item a {
  font-size: 28rpx;
  font-weight:400;
  color: #333;
} */

.tab .textUnActive {
  font-weight: 400;
  color: #333;
  font-family: PingFangSC-Regular, PingFang SC;
}

.tab .textActive {
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #333;
}

.tab .active {
  position: absolute;
  top: 66rpx;
  margin-left: 14rpx;
  width: 32rpx;
  height: 4rpx;
  background: rgba(250, 190, 0, 1);
}

.goods {
  border-top: 4rpx solid #f5f5f5;
  display: flex;
  position: absolute;
  width: 100%;
  top: 330rpx;
  bottom: 92rpx;
  overflow: hidden;
  font-family: "微软雅黑";
}

.goods .menu-wrapper {
  flex: 0 0 164rpx;
  width: 164rpx;
  background: rgba(0, 0, 0, 0.06);
}

.goods .menu-wrapper .menu-item {
  text-align: center;
  display: table;
  width: 164rpx;
  color: rgba(102, 102, 102, 1);
  height: 88rpx;
  line-height: 28rpx;
}

.goods .menu-wrapper .menu-item:current {
  position: relative;
  z-index: 10;
  margin-top: -2rpx;
  background: #fff;
  font-weight: 700;
}

.goods .menu-wrapper .menu-item:current .text {
  border: none;
}

.goods .menu-wrapper .menu-item .icon {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  vertical-align: top;
  margin-right: 8rpx;
  background-size: 24rpx 24rpx;
  background-repeat: no-repeat;
}

.goods .menu-wrapper .menu-item .text {
  display: table-cell;
  width: 112rpx;
  vertical-align: middle;
  position: relative;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  letter-spacing: 1pt;
  line-height: 34rpx;
}

.goods .menu-wrapper .menu-item-selected {
  background: white;
  font-size: 24rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
  margin-top: -2rpx;
}

.goods .foods-wrapper {
  flex: 1;
}

.goods .foods-wrapper .food-list .title {
  padding-left: 20rpx;
  width: 100%;
  height: 68rpx;
  font-size: 26rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 68rpx;
}

.goods .foods-wrapper .food-list .food-item {
  display: flex;
  margin: 0rpx 16rpx 24rpx 20rpx;
  position: relative;
  /* border-bottom: 2rpx solid rgba(7, 17, 27, 0.1); */
}

.goods .foods-wrapper .food-list .food-item:last-child {
  border: none;
  margin-bottom: 0rpx;
}

.goods .foods-wrapper .food-list .food-item .icon {
  flex: 0 0 184rpx;
  margin-right: 16rpx;
  width: 184rpx;
  height: 184rpx;
  background-color: #d3d3d3;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.goods .foods-wrapper .food-list .food-item .content {
  flex: 1;
}

.goods .foods-wrapper .food-list .food-item .content .name {
  width: 340rpx;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goods .foods-wrapper .food-list .food-item .content .extra {
  height: 32rpx;
  font-size: 22rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 32rpx;
  margin-top: 16rpx;
}

.goods .foods-wrapper .food-list .food-item .content .extra .count {
  margin-right: 24rpx;
}

.goods .foods-wrapper .food-list .food-item .content .price {
  position: absolute;
  width: auto;
  height: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  line-height: 32rpx;
}

.goods .foods-wrapper .food-list .food-item .content .benefit-h {
  color: #f52340;
  bottom: 32rpx;
}

.goods .foods-wrapper .food-list .food-item .content .benefit-l {
  color: rgba(51, 51, 51, 1);
  bottom: 0rpx;
}

.goods .foods-wrapper .food-list .food-item .content .price .now {
  margin-right: 16rpx;
  font-size: 28rpx;
}

.goods .foods-wrapper .food-list .food-item .content .benefit {
  width: auto;
  height: 26rpx;
  font-size: 16rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 26rpx;
  background: rgba(245, 35, 64, 0.08);
  border-radius: 4rpx;
  position: absolute;
  box-sizing: border-box;
  bottom: 0rpx;
  padding: 2rpx 8rpx;
}

.goods .foods-wrapper .food-list .food-item .content .benefit text {
  color: red;
}

.goods .foods-wrapper .food-list .food-item .content .price .old {
  font-size: 20rpx;
  color: rgb(147, 153, 159);
  text-decoration: line-through;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper {
  position: absolute;
  right: 0;
  bottom: 0rpx;
}

/* cartControl 样式 */

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl {
  font-size: 0;
  height: 50rpx;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .font {
  width: 50rpx;
  height: 50rpx;
  margin-top: 11rpx;
}

.goods .foods-wrapper .food-list .food-item .content .cartControl-wrapper .cartControl .cart-count {
  display: inline-block;
  font-size: 22rpx;
  line-height: 70rpx;
  min-width: 10rpx;
  vertical-align: top;
  padding-top: 2rpx;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

/* shopcart 样式 */

.shopCart {
  position: fixed;
  left: 0rpx;
  bottom: 0rpx;
  z-index: 50;
  width: 100%;
  height: 88rpx;
}

.shopCart .content {
  display: flex;
  background: #000;
}

.shopCart .content .content-left {
  flex: 1;
}

.shopCart .content .content-left .logo-wrapper {
  display: inline-block;
  position: relative;
  top: -20rpx;
  margin: 0 24rpx;
  padding: 12rpx;
  width: 106rpx;
  height: 106rpx;
  box-sizing: border-box;
  vertical-align: top;
  border-radius: 50%;
  background: #000;
}

.shopCart .content .content-left .logo-wrapper .num {
  position: absolute;
  top: 12rpx;
  right: 16rpx;
  width: 28rpx;
  height: 28rpx;
  line-height: 28rpx;
  text-align: center;
  border-radius: 32rpx;
  font-size: 20rpx;
  font-weight: 700;
  color: #fff;
  background: #ff5850;
}

.shopCart .content .content-left .logo-wrapper .logo {
  width: 100%;
  height: 100%;
  text-align: center;
  border-radius: 50%;
  background: #000;
}

.shopCart .content .content-left .logo-wrapper .logo.highlight {
  background: #fabe00;
}

.shopCart .content .content-left .logo-wrapper .logo .img {
  margin-top: 16rpx;
  width: 50rpx;
  height: 50rpx;
}

.shopCart .content .content-left .price {
  display: inline-block;
  vertical-align: top;
  margin-top: 24rpx;
  padding-right: 24rpx;
  box-sizing: border-box;
  border-right: 2rpx solid rgba(255, 255, 255, 0.1);
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(255, 255, 255, 1);
  line-height: 40rpx;
}

.shopCart .content .content-left .price.highlight {
  color: #fff;
}

.shopCart .content .content-left .price.highlight .specialPrice {
  margin-left: 10rpx;
  height: 32rpx;
  font-size: 22rpx;
  color: rgba(153, 153, 153, 1);
  line-height: 32rpx;
  text-decoration: line-through;
}

.shopCart .content .content-left .desc {
  display: inline-block;
  vertical-align: top;
  line-height: 40rpx;
  margin-left: 24rpx;
  margin-top: 24rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(255, 255, 255, 0.6);
}

.shopCart .content .content-right {
  flex: 0 0 192rpx;
  width: 192rpx;
}

.shopCart .content .content-right .pay {
  height: 88rpx;
  line-height: 88rpx;
  text-align: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.shopCart .content .content-right .pay.enough {
  background: #fabe00;
}

.shopCart .ball-container .ball {
  position: fixed;
  left: 64rpx;
  bottom: 44rpx;
  z-index: 200;
  transition: all 0.6s cubic-bezier(0.49, -0.29, 0.75, 0.41);
}

.shopCart .ball-container .ball .inner {
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  background: rgb(0, 160, 220);
  transition: all 0.4s linear;
}

.shopCart .shopcart-list {
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  transform: translate3d(0, -100%, 0);
}

.shopCart .shopcart-list :fade-enter-active, :fade-leave-active {
  transition: all 0.5s transform translate3d(0, -100%, 0);
}

.shopCart .shopcart-list :fade-enter, :fade-leave-active {
  transform: translate3d(0, 0, 0);
}

.shopCart .shopcart-list .special-header {
  width: 100%;
  height: 84rpx;
  background: rgba(255, 248, 226, 1);
  display: flex;
  flex-direction: row;
  text-align: center;
  justify-content: center;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(245, 35, 64, 1);
  line-height: 84rpx;
}

.shopCart .shopcart-list .list-header {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 36rpx;
  background: #fff;
  box-sizing: border-box;
}

.shopCart .shopcart-list .list-header .line {
  box-sizing: border-box;
  height: 80rpx;
  border-bottom: 2rpx solid rgba(7, 17, 27, 0.1);
}

.shopCart .shopcart-list .list-header .title {
  float: left;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.shopCart .shopcart-list .list-header .empty {
  float: right;
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
}

.shopCart .shopcart-list .list-content {
  padding: 0 20rpx 0 32rpx;
  max-height: 666rpx;
  overflow: hidden;
  background: #fff;
  box-sizing: border-box;
}

.shopCart .shopcart-list .list-content .shopcart-food {
  width: 100%;
  height: 104rpx;
  position: relative;
  margin: 32rpx 0 8rpx 0;
  display: flex;
  flex-direction: row;
  box-sizing: border-box;
  /*border-top: 1px solid rgba(7,17,27,0.1);*/
}

.shopCart .shopcart-list .list-content .shopcart-food .img {
  width: 104rpx;
  height: 104rpx;
  background-color: #d3d3d3;
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.shopCart .shopcart-list .list-content .shopcart-food:last-child {
  margin-bottom: 32rpx;
}

/* .shopCart .shopcart-list .list-content .shopcart-food image {
  width: 104rpx;
  height: 104rpx;
} */

.shopCart .shopcart-list .list-content .shopcart-food .itemDetail {
  box-sizing: border-box;
  flex: 1;
  margin-left: 24rpx;
  display: flex;
  flex-direction: column;
}

.shopCart .shopcart-list .list-content .shopcart-food .name {
  margin-left: 5rpx;
  width: 400rpx;
  height: 36rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 36rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.shopCart .shopcart-list .list-content .shopcart-food .spec {
  margin-left: 5rpx;
  min-width: 100rpx;
  height: 36rpx;
  font-size: 20rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(102, 102, 102, 1);
  line-height: 36rpx;
}

.shopCart .shopcart-list .list-content .shopcart-food .price {
  box-sizing: border-box;
  width: 100%;
  flex: 1;
  height: 35rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 35rpx;
}

.shopCart .shopcart-list .list-content .shopcart-food .price .special {
  margin-left: 5rpx;
  font-size: 22rpx;
  text-decoration: line-through;
}

.shopCart .shopcart-list .list-content .shopcart-food .cartControl-wrapper {
  position: absolute;
  right: 0rpx;
  bottom: -10rpx;
}

/* cartControl 样式 */

.goods .shopCart  .cartControl-wrapper .cartControl {
  font-size: 0;
  height: 50rpx;
}

.goods .shopCart  .cartControl-wrapper .cartControl .font {
  width: 50rpx;
  height: 50rpx;
}

.goods .shopCart  .cartControl-wrapper .cartControl .cart-decrease {
  display: inline-block;
  font-size: 32rpx;
  line-height: 44rpx;
}

.goods .shopCart  .cartControl-wrapper .cartControl .cart-decrease,
.cart-add :inner-enter-active, :inner-leave-active {
  transition: all 0.4s linear;
  transform: rotate(0);
}

.goods .shopCart  .cartControl-wrapper .cartControl .cart-decrease,
.cart-add :inner-enter, :inner-leave-active {
  opacity: 0 transform rotate(180deg);
}

.goods .shopCart  .cartControl-wrapper .cartControl .cart-count {
  display: inline-block;
  font-size: 22rpx;
  margin-top: 2rpx;
  line-height: 50rpx;
  width: 24rpx;
  vertical-align: top;
  text-align: center;
  color: rgba(51, 51, 51, 1);
}

.goods .shopCart  .cartControl-wrapper .cartControl .cart-add {
  display: inline-block;
  padding-top: 1rpx;
  line-height: 44rpx;
  font-size: 34rpx;
  vertical-align: top;
}

/* head */

.header {
  position: relative;
  color: #fff;
  overflow: hidden;
  background: rgba(7, 17, 27, 0.5);
}

.header .content-wrapper {
  position: relative;
  padding: 48rpx 24rpx 36rpx 48rpx;
  font-size: 0;
}

.header .content-wrapper .avatar {
  display: inline-block;
  vertical-align: top;
}

.header .content-wrapper .avatar img {
  border-radius: 4rpx;
}

.header .content-wrapper .content {
  display: inline-block;
  font-size: 28rpx;
  margin-left: 32rpx;
}

.header .content-wrapper .content .title {
  margin: 4rpx 0 16rpx 0;
}

.header .content-wrapper .content .title .brand {
  display: inline-block;
  vertical-align: top;
  width: 60rpx;
  height: 36rpx;
  /*bg-image('brand')*/
  background-size: 60rpx 36rpx;
  background-repeat: no-repeat;
}

.header .content-wrapper .content .title .name {
  margin-left: 12rpx;
  font-size: 32rpx;
  line-height: 36rpx;
  font-weight: bold;
}

.header .content-wrapper .content .description {
  margin-bottom: 20rpx;
  line-height: 24rpx;
  font-size: 24rpx;
}

.header .content-wrapper .content .support .icon {
  display: inline-block;
  width: 24rpx;
  height: 24rpx;
  vertical-align: top;
  margin-right: 8rpx;
  background-size: 24rpx 24rpx;
  background-repeat: no-repeat;
  /*&.decrease
            bg-image('decrease_1')
          &.discount
            bg-image('discount_1')
          &.guarantee
            bg-image('guarantee_1')
          &.invoice
            bg-image('invoice_1')
          &.special
            bg-image('special_1')*/
}

.header .content-wrapper .content .support .text {
  line-height: 24rpx;
  font-size: 24rpx;
}

.header .content-wrapper .content .supports-count {
  position: absolute;
  right: 24rpx;
  bottom: 28rpx;
  padding: 0 16rpx;
  height: 48rpx;
  line-height: 48rpx;
  border-radius: 24rpx;
  background: rgba(0, 0, 0, 0.2);
  text-align: content;
}

.header .content-wrapper .content .supports-count .count {
  font-size: 20rpx;
  vertical-align: top;
}

.header .content-wrapper .content .supports-count .icon {
  margin-left: 4rpx;
  line-height: 48rpx;
  font-size: 20rpx;
}

.header .bulletin-wrapper {
  position: relative;
  background-color: rgba(7, 17, 27, 0.2);
  height: 56rpx;
  line-height: 56rpx;
  padding: 0 44rpx 0 24rpx;
  white-space: normal;
  overflow: hidden;
  text-overflow: ellipsis;
}

.header .bulletin-wrapper .bulletin-title {
  display: inline-block;
  vertical-align: top;
  margin-top: 16rpx;
  width: 44rpx;
  height: 24rpx;
  /*bg-image('bulletin')*/
  background-size: 44rpx 24rpx;
  background-repeat: no-repeat;
}

.header .bulletin-wrapper .bulletin-text {
  vertical-align: top;
  margin: 0 8rpx;
  font-size: 20rpx;
}

.header .bulletin-wrapper .icon {
  position: absolute;
  font-size: 40rpx;
  right: 24rpx;
  top: 0rpx;
}

.header .background {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
  filter: blur(20rpx);
}

.header .background .detail {
  position: fixed;
  z-index: 100;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  overflow: auto;
  background: rgba(7, 17, 27, 0.8);
  -webkit-backdrop-filter: blur(20rpx);
}

.list-mask {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  z-index: 40;
  /*backdrop-filter: blur(10px);*/
  -webkit-backdrop-filter: 1;
  background: rgba(0, 0, 0, 0.3);
}

.list-mask :fade-enter-active, :fade-leave-active {
  opacity: 1;
  transition: all 0.5s;
  background: rgba(7, 17, 27, 0.6);
}

.food-list :fade-enter, :fade-leave-active {
  opacity: 0;
  background: rgba(7, 17, 27, 0);
}

/* 字体图标 */

@font-face {
  font-family: 'iconfont';
  src: url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
         url('iconfont.ttf') format('truetype');
  font-weight: 500;
  font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-zuoyoujiantou:before {
  content: "\e600";
}

.icon-gouwuche:before {
  content: "\e601";
}

.icon-jian:before {
  content: "\e629";
}

