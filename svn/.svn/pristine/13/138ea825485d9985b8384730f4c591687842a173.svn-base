/* pages/goodss/goodss.wxss */

page {
  background: #fff;
}

.navigation-bar {
  border-top: 1rpx solid #dcdcdc;
  position: relative;
  width: 100%;
  height: 136rpx;
  color: #fff;
  overflow: hidden;
}

.imm {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  filter: blur(5px);
  z-index: -1;
}

