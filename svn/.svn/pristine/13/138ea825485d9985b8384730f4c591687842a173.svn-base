::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

.mask {
  width: 750rpx;
  height: 100%;
  position: fixed;
  background: #000;
  opacity: 0.3;
  top: 0;
  z-index: 99;
}

/* 规格弹窗 */

.dialog {
  width: 750rpx;
  max-height: 900rpx;
  background: #fff;
  position: fixed;
  bottom: 0;
  z-index: 100;
}

.dialog .good {
  padding: 0 24rpx;
  margin-bottom: 60rpx;
  box-sizing: border-box;
  display: flex;
  position: relative;
}

.dialog .good .cancel-icon {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
}

.dialog .good .spec-img {
  width: 200rpx;
  height: 200rpx;
  border-radius: 4rpx;
  position: absolute;
  top: -56rpx;
  /* border: 1px solid #ededed; */
}

.dialog-goods-info {
  height: 144rpx;
  margin-left: 224rpx;
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 32rpx 0;
}

.dialog-spec-price {
  font-size: 32rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 32rpx;
  display: flex;
  flex-direction: row;
}

.dialog-spec-yprice {
  height: 32rpx;
  font-size: 32rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(245, 35, 64, 1);
  line-height: 32rpx;
}

.dialog-spec-xprice {
  height: 32rpx;
  margin-left: 5rpx;
  font-size: 28rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(153, 153, 153, 1);
  line-height: 32rpx;
  text-decoration: line-through;
}

.dialog-spec {
  margin-top: 16rpx;
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

.dialog .spec {
  box-sizing: border-box;
  max-height: 400rpx;
  overflow: scroll;
}

.spec-title {
  line-height: 90rpx;
  font-size: 28rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
}

.spec-value {
  display: flex;
  flex-wrap: wrap;
}

.spec-value-item {
  padding: 0 24rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  border: 2rpx solid rgba(153, 153, 153, 1);
  font-size: 24rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
  margin-right: 32rpx;
  margin-bottom: 16rpx;
  position: relative;
  box-sizing: border-box;
}

.dialog .spec .checked {
  border: 1px solid rgba(0, 0, 0, 1);
  color: #333;
}

.dialog .spec .checked::after {
  content: "";
  position: absolute;
  width: 0;
  height: 0;
  border-right: 16rpx solid transparent;
  border-left: 16rpx solid transparent;
  border-top: 16rpx solid #fabe00;
  transform: rotate(-45deg);
  right: -10rpx;
  bottom: -2rpx;
}

.dialog .spec .disabled {
  border: none;
  color: #999;
  background: rgba(0, 0, 0, 0.1);
}

.spec-main, .spec-sub {
  padding: 0 24rpx;
}

.dialog .num {
  padding: 0 24rpx;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  font-family: PingFang-SC;
  font-weight: 500;
  color: rgba(102, 102, 102, 1);
}

/*主容器*/

.stepper {
  width: 180rpx;
  height: 44rpx;
  /*给主容器设一个边框*/
  border: 1px solid #ededed;
  border-radius: 8rpx;
  vertical-align: middle;
}

/*加号和减号*/

.stepper text {
  width: 50rpx;
  line-height: 44rpx;
  text-align: center;
  float: left;
  box-sizing: border-box;
  color: #999;
}

/*数值*/

.stepper input {
  box-sizing: border-box;
  width: 80rpx;
  height: 44rpx;
  float: left;
  margin: 0 auto;
  text-align: center;
  font-size: 24rpx;
  /*给中间的input设置左右边框即可*/
  border-left: 1px solid #ededed;
  border-right: 1px solid #ededed;
  color: #666;
}

.dialog .button {
  width: 702rpx;
  height: 88rpx;
  margin: 44rpx 24rpx;
  line-height: 88rpx;
  text-align: center;
  background: rgba(250, 190, 0, 1);
  border-radius: 8px;
  font-size: 36rpx;
  font-family: PingFangSC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
}

.shop {
  border-top: 4rpx solid #f5f5f5;
  display: flex;
  position: absolute;
  width: 100%;
  height: auto;
  top: 330rpx;
  overflow: hidden;
  font-family: "微软雅黑";
  flex-direction: column;
}

.shop .shopInfo {
  box-sizing: border-box;
  width: 100%;
  height: auto;
  padding: 10rpx 24rpx 24rpx 24rpx;
}

.shop .shopInfo .title {
  width: 100%;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: rgba(51, 51, 51, 1);
  line-height: 40rpx;
}

.shop .shopInfo .text {
  width: 100%;
  height: 100%;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: rgba(51, 51, 51, 1);
  line-height: 34rpx;
  text-align: justify;
  text-justify: inter-ideograph;
  display: inline-block;
  margin-top: 10rpx;
}

.pay-mask {
  width: 100%;
  height: 100%;
  background: #fff;
  opacity: 0;
  z-index: 9000;
  position: fixed;
  top: 0;
  left: 0;
}
